{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "blogDate": {
      "type": "date"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tags": {
      "via": "blogtag",
      "collection": "tags",
      "dominant": true
    }
  }
}
